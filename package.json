{
    "name": "org-struct",
    "version": "0.0.1",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon src/main.ts",
        "build:clean": "rimraf .next",
        "build:ui": "npx next build",
        "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --extensions \".js\"",
        "build": "yarn build:clean && yarn build:ui && yarn build:server",
        "start": "cross-env NODE_ENV=production node .next/production-server/src/main.js",
        "format": "prettier --write \"src/**/*.ts\"",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "migration:run": "yarn typeorm migration:run -d ./.next/production-server/config/typeorm.js",
        "migration:revert": "yarn typeorm migration:revert -d ./.next/production-server/config/typeorm.js"
    },
    "dependencies": {
        "@nestjs/common": "^8.4.4",
        "@nestjs/config": "^3.2.0",
        "@nestjs/core": "^8.4.4",
        "@nestjs/platform-express": "^8.4.4",
        "@nestjs/swagger": "^7.3.0",
        "@nestjs/typeorm": "^10.0.2",
        "cross-env": "^7.0.3",
        "eslint-plugin-unused-imports": "^3.1.0",
        "nest-next": "^9.6.0",
        "next": "12.1.5",
        "pg": "^8.11.3",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.5",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.9",
        "@types/node": "^17.0.23",
        "@types/react": "^18.0.4",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    }
}
